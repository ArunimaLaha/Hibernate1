<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "src/hibernate-mapping-3.0.dtd">
 
          <hibernate-mapping>
         <class name="com.pack.model.Employee" table="Employee100" dynamic-update="true">
         <id name="id" column="Emp_id">
         <generator class="assigned"></generator>
         </id>
         <property name="name" column="Emp_name"></property>
         <property name="salary"></property>
         <property name="dob"></property>
         <property name="designation"></property>
         </class>
          </hibernate-mapping>